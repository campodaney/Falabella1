@model List<Falabella.Models.ViewModels.Cliente.ListClienteViewModels>
@{
    ViewBag.Title = "Clientes";
}

<h2>@ViewBag.Title</h2>

<div class="row" style="text-align:right;padding:5px;">
    <a href="/Cliente/Nuevo" class="btn btn-primary">Nuevo</a>

</div>
<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>#</th>
                <th>TipoDocumento</th>
                <th>Documento</th>
                <th>Nombre</th>
                <th></th>
            </tr>
            @foreach (var oElemento in Model)
            {
        <tr>
            <td>@oElemento.idCliente</td>
            <td>@oElemento.idTipoDocumento</td>
            <td>@oElemento.documento</td>
            <td>@oElemento.nombre</td>
            <td>
                @*<a class="btn btn-default" href="~/Compania/Editar/@oElemento.idCompania">Editar</a>*@
                <a class="btn btn-default" href="@Url.Action("Editar", "Cliente", new { udCliente = @oElemento.idCliente })">Editar</a>

            </td>
        </tr>
            }
        </table>
    </div>
</div>



